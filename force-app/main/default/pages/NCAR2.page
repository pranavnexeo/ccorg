<apex:pageblockButtons >
<apex:commandbutton value="Save Draft" action="{!savedraftincident}"/>
<apex:commandbutton value="Save and Submit" action="{!createsubmitincident}"/>
<apex:commandbutton value="Cancel" action="{!cancel}"/>
</apex:pageblockButtons>

<apex:pageblocksection title="Case Information">
<apex:outputfield value="{!mycase.ownerid}"/>
