/*to change start date and expiration date */
    function fnChngDate(){
         var expDate = $('#exdate').val().split('/');
         var newdate = new Date(expDate);
         newdate.setDate(newdate.getDate() + 1);
         var startdate = (newdate.getMonth()+1) + '/' + newdate.getDate()+ '/' + newdate.getFullYear();
         var nextExpdate = new Date(expDate);
         nextExpdate.setDate(nextExpdate.getDate()+ 366);
         var nextdate = (nextExpdate.getMonth()+1) + '/' + nextExpdate.getDate()+ '/' + nextExpdate.getFullYear();
         $('#edatenext').val(startdate);
         $('#exdatenext').val(nextdate);
    }
/*to change start date and expiration date ends */
    
